<NotepadPlus>
	<functionList>
		<!-- ========================================================= [ PL/SQL ] -->
		<!--parser id="plsql_function" displayName="PL/SQL" commentExpr="((\/\*([^\*]|\*[^\/])*\*\/)|(--.*?$))" reactivate when the bug on comments is fixed -->
		<parser id="plsql_function" displayName="PL/SQL" commentExpr="">
			<function
				mainExpr="(?is)^[ \t]*(CREATE([\s]+OR[\s]+REPLACE)?[\s]+)?(EDITIONABLE\s+)?((PACKAGE([\s]+BODY)?)|(PROCEDURE)|(FUNCTION))([\s]+[A-Z]\w*|\s*\x22[A-Z]\w*\x22)([\s]*(?'open'\().*?(\)))?(([\s]*;)|([\s]*([ia]s)\s)|([\s]+(RETURN)([\s]+[\w%\.]+)+(([\s]*;)|([\s]+([ia]s)))))"
				displayMode="$className->$functionName"
			>
				<className>
					<nameExpr expr="(?is)((PACKAGE([\s]+BODY)?))([\s]+[A-Z]\w*|\s*\x22[A-Z]\w*\x22)([\s]+[ai]s[\s]*)?"/>
				</className>
				<functionName>
					<nameExpr expr="(?is)([\s]+[A-Z]\w*|\s*\x22[A-Z]\w*\x22)([\s]*(?'open'\().*?(\)))?(([\s]*;)|([\s]*([ia]s)\s)|([\s]+(RETURN)([\s]+[\w%\.]+)+(([\s]*;)|([\s]+([ia]s)))))"/>
					<nameExpr expr="(?is)([\s]+[A-Z]\w*|\s*\x22[A-Z]\w*\x22)([\s]*\([^\)]*\))?(([\s]+(RETURN)([\s]+[\w%\.]+)+))*"/>
				</functionName>
			</function>
		</parser>
	</functionList>
</NotepadPlus>
